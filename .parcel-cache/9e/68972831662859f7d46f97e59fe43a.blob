body{background:#eee}*{box-sizing:border-box;padding:0;margin:0}@keyframes x{form{transform:rotate(0deg)}to{transform:rotate(1turn)}}#八卦{width:400px;height:400px;border-radius:50%;position:relative;overflow:hidden;animation:x 10s linear infinite;box-shadow:0 0 5px 3px rgba(0,0,0,.75)}@media(max-width:500px){#八卦{width:200px;height:200px}}#八卦>div:first-child{width:50%;height:100%;position:absolute;left:0;background:#000}#八卦>div:nth-child(2){height:100%;width:50%;position:absolute;right:0;background:#fff}#八卦>div:nth-child(3){height:200px;width:200px;position:absolute;left:50%;margin-left:-100px;border-radius:50%;background:#000}@media(max-width:500px){#八卦>div:nth-child(3){height:100px;width:100px;margin-left:-50px}}#八卦>div:nth-child(4){height:200px;width:200px;position:absolute;left:50%;bottom:0;margin-left:-100px;border-radius:50%;background:#fff}@media(max-width:500px){#八卦>div:nth-child(4){height:100px;width:100px;bottom:0;margin-left:-50px}}#八卦>div:nth-child(5){height:50px;width:50px;position:absolute;left:50%;top:75px;margin-left:-25px;border-radius:50%;background:#fff}@media(max-width:500px){#八卦>div:nth-child(5){height:25px;width:25px;top:37.5px;margin-left:-12.5px}}#八卦>div:nth-child(6){height:50px;width:50px;position:absolute;left:50%;bottom:75px;margin-left:-25px;border-radius:50%;background:#000}@media(max-width:500px){#八卦>div:nth-child(6){height:25px;width:25px;bottom:37.5px;margin-left:-12.5px}}#八卦-wrapper{height:100vh;display:flex;justify-content:center;align-items:center;flex-direction:column}#八卦-描述{margin-top:1em}

